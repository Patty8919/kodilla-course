DROP FUNCTION IF EXISTS BESTSELLERS;

DELIMITER $$

CREATE FUNCTION BESTSELLERS(RENT_ID int) returns BOOLEAN DETERMINISTIC
BEGIN
	DECLARE result BOOLEAN DEFAULT TRUE;
    IF RENT_ID > 2 THEN
		SET result = TRUE;
    ELSE
		SET result = FALSE;
    END IF;
    RETURN result;
END $$

DELIMITER ;

SELECT BESTSELLERS(2) AS BESTSELLER