DROP FUNCTION IF EXISTS BESTSELLER;

DELIMITER $$

CREATE FUNCTION BESTSELLER(BOOKSRENT INT) returns boolean DETERMINISTIC
BEGIN
	DECLARE result boolean DEFAULT TRUE ;
    IF BOOKSRENT >= 2 THEN
		SET result = TRUE;
    ELSE
		SET result = FALSE;
    END IF;
    RETURN result;
END $$

DELIMITER ;

SELECT BESTSELLER(0) AS LEVEL